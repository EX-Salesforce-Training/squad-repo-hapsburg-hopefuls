<aura:component controller="BidHistoryController" implements="force:hasRecordId,flexipage:availableForAllPageTypes" access="global">
    <lightning:messageChannel type="SampleMessageChannel__c" aura:id="SampleMessageChannel" onMessage="{!c.handleMessage}" scope="APPLICATION" />

    <aura:attribute name="data" type="Object" />
    <aura:attribute name="columns" type="List" />
    <aura:attribute name="recordId" type="Id" />
    <aura:handler name="init" value="{! this }" action="{! c.doInit }" />
    <!--<aura:handler event="c:ResultApplicationEvent" action="{!c.onResultHandler}" />-->

    <lightning:card title="BidHistory">
        <div class="slds-var-p-around_small">
            <aura:if isTrue="{!v.recordIdFromMsgChannel}">
                <lightning:datatable recordID="{!v.recordIdFromMsgChannel}" data="{! v.data }" columns="{! v.columns }" keyField="id" hideCheckboxColumn="true" />
                <aura:set attribute="else">Record-id not found</aura:set>
            </aura:if>
        </div>
    </lightning:card>


</aura:component>